<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane fx:id="sceneDashboard" prefHeight="590.0" prefWidth="843.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.schoolmanagementsystem.DashboardController">
   <children>
      <HBox layoutY="1.0" prefHeight="50.0" prefWidth="847.0">
         <children>
            <Button fx:id="studentDash" mnemonicParsing="false" onAction="#switchToStudent" prefHeight="50.0" prefWidth="133.0" text="Students" />
            <Button fx:id="professorDash" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#switchToProfessor" prefHeight="50.0" prefWidth="136.0" text="Professor" />
            <Button fx:id="scheduleDash" layoutX="143.0" layoutY="10.0" mnemonicParsing="false" onAction="#switchToSchedule" prefHeight="50.0" prefWidth="136.0" text="Schedule" />
         </children>
      </HBox>
      <Button fx:id="logoutB" layoutX="14.0" layoutY="533.0" mnemonicParsing="false" onAction="#closeDashboard" prefHeight="43.0" prefWidth="80.0" text="Log out" />
      <Button fx:id="addStudent" layoutX="723.0" layoutY="533.0" mnemonicParsing="false" onAction="#addStudent" prefHeight="43.0" prefWidth="110.0" text="Add Student" />
      <Button fx:id="addProfessor" layoutX="594.0" layoutY="533.0" mnemonicParsing="false" onAction="#addProfessor" prefHeight="43.0" prefWidth="110.0" text="Add Professor" />
      <Button fx:id="addCourse" layoutX="465.0" layoutY="533.0" mnemonicParsing="false" onAction="#addCourse" prefHeight="43.0" prefWidth="110.0" text="Add Course" />
      <Button fx:id="addModule" layoutX="339.0" layoutY="533.0" mnemonicParsing="false" onAction="#addModule" prefHeight="43.0" prefWidth="110.0" text="Add Module" />
   </children>
</AnchorPane>
